{
  "entryPointStrategy": "packages",
  "entryPoints": ["api/", "ui/"],
  "packageOptions": {
    "includeVersion": true,
    "entryPoints": ["src/index.ts"],
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/dist/**",
      "**/docs/**",
      "**/*.spec.ts"
    ],
    "excludeExternals": true
  },
  "logLevel": "Verbose"
}

